PROCEDURE insertion_sort (arr)
VAR
    j,key := INTEGER;
    
BEGIN
    WHILE (J>=0 and arr[j]>key) DO
        arr [j+1]=arr[j]
        j=j+1
    END_WHILE
    arr[j+1]=key
END
ALGORITHM tri simple
VAR
    i, j,key:=INTEGER;
    arr:=ARRAY_OF INTEGER;
BEGIN
    insertion_sort(arr)
    n:=length(arr)
    FOR i FROM 1 TO n DO
        key:=arr[i];
        j:=i-1;
    END_FOR
END